{
  "name": "rocketgraph/php-sdk",
  "license": "MIT",
  "type": "project",
  "description": "Rocketgraph PHP SDK for report development",
  "autoload": {
    "psr-4": {
      "": "src/",
      "RG\\": "core/",
      "RAM\\": "reports-lib/",
      "Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=5.5.0",
    "symfony/symfony": "2.7.*",
    "sensio/buzz-bundle": "dev-master",
    "guzzlehttp/guzzle": "^6.2"
  },
  "scripts": {
    "post-install-cmd": [
      "RG\\Composer\\ScriptHandler::createFolders",
      "RG\\Composer\\ScriptHandler::setupConfig"
    ],
    "post-update-cmd": [
      "RG\\Composer\\ScriptHandler::createFolders",
      "RG\\Composer\\ScriptHandler::setupConfig"
    ]
  }
}
